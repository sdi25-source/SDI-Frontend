{
  "name": "sdi-frontend",
  "version": "2.0.0",
  "private": true,
  "description": "Description for Sdi Frontend",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch'",
    "ci:frontend:build": "npm run webapp:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "clean-www": "rimraf target/classes/static/",
    "cleanup": "rimraf target/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "prettier:check": "prettier --check \"{,src/**/,.blueprint/**/}*.{md,json,yml,js,cjs,mjs,ts,cts,mts,html,vue,css,scss}\"",
    "prettier:format": "prettier --write \"{,src/**/,.blueprint/**/}*.{md,json,yml,js,cjs,mjs,ts,cts,mts,html,vue,css,scss}\"",
    "serve": "npm run start --",
    "start": "npm run webapp:dev --",
    "start-tls": "npm run webapp:dev -- --env.tls",
    "pretest": "npm run lint",
    "test": "npm run vitest-run --",
    "test:watch": "npm run vitest",
    "vite-build": "vite build",
    "vite-serve": "vite",
    "vitest": "vitest",
    "vitest-run": "vitest --run --coverage",
    "watch": "concurrently npm:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev --",
    "webapp:build:dev": "npm run vite-build",
    "webapp:build:prod": "npm run vite-build",
    "webapp:dev": "npm run vite-serve",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod --",
    "webapp:serve": "npm run vite-serve",
    "webapp:test": "npm run test --"
  },
  "config": {
    "default_environment": "prod"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/vue-fontawesome": "3.0.8",
    "@vue/compat": "3.5.13",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "12.5.0",
    "axios": "^1.9.0",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.23.1",
    "chart.js": "^4.4.9",
    "dayjs": "1.11.13",
    "deepmerge": "4.3.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-vue-next": "^0.511.0",
    "pinia": "2.3.1",
    "sass-loader": "^16.0.5",
    "vue": "3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-i18n": "^11.1.3",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "9.20.0",
    "@pinia/testing": "0.1.7",
    "@tsconfig/node18": "18.2.4",
    "@types/node": "20.11.25",
    "@types/sinon": "17.0.3",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitest/coverage-v8": "3.0.5",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.20",
    "axios-mock-adapter": "2.1.0",
    "concurrently": "9.1.2",
    "eslint": "9.20.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-vue": "9.32.0",
    "flush-promises": "1.0.2",
    "generator-jhipster": "^8.11.0",
    "happy-dom": "17.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "numeral": "2.0.6",
    "postcss-import": "16.1.0",
    "postcss-url": "10.1.3",
    "prettier": "3.4.2",
    "prettier-plugin-packagejson": "2.5.8",
    "rimraf": "5.0.8",
    "sass": "^1.64.2",
    "sinon": "19.0.2",
    "swagger-ui-dist": "5.18.3",
    "typescript": "5.7.3",
    "typescript-eslint": "8.23.0",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "2.2.0",
    "vitest": "3.0.5",
    "vitest-sonar-reporter": "2.0.0",
    "wait-on": "8.0.2"
  },
  "engines": {
    "node": ">=22.13.1"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
